{
	"name": "react-boilerplate",
	"version": "1.0.0",
	"description": "a basic and opininated boilerplate for a react app",
	"main": "index.js",
	"scripts": {
		"start": "webpack-dev-server --open --config webpack.dev.js",
		"test": "jest",
		"test:watch": "jest --watchAll",
		"test:cover": "npm run test -- --coverage ",
		"watch": "webpack --watch",
		"build": "webpack --config webpack.prod.js ",
		"dashboard": "webpack-dashboard -- webpack-dev-server --open --config webpack.dev.js",
		"server": "json-server --watch server/db.json --port 3004",
		"lint": "eslint ./src"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/muneneevans/react-js-bolierplate.git"
	},
	"keywords": [
		"boilerplate"
	],
	"author": "Evans Munene",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/muneneevans/react-js-bolierplate/issues"
	},
	"homepage": "https://github.com/muneneevans/react-js-bolierplate#readme",
	"dependencies": {
		"react-dom": "^16.3.2",
		"react-redux": "^5.0.5",
		"react-router": "^4.1.1",
		"react-router-dom": "^4.1.1",
		"redux": "^3.7.1",
		"redux-thunk": "^2.2.0",
		"seamless-immutable": "^7.1.2"
	},
	"devDependencies": {
		"babel": "^6.23.0",
		"babel-core": "^6.24.1",
		"babel-eslint": "^7.2.3",
		"babel-jest": "^22.4.1",
		"babel-loader": "^7.0.0",
		"babel-plugin-dynamic-import-node": "^1.2.0",
		"babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
		"babel-plugin-transform-react-jsx": "^6.24.1",
		"babel-polyfill": "^6.23.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-2": "^6.24.1",
		"babili-webpack-plugin": "^0.1.2",
		"browser-sync": "^2.23.6",
		"browser-sync-webpack-plugin": "^1.2.0",
		"clean-webpack-plugin": "^0.1.17",
		"css-loader": "^0.28.4",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-15": "^1.0.5",
		"enzyme-adapter-react-16": "^1.1.1",
		"eslint": "^6.8.0",
		"eslint-plugin-react": "^7.5.1",
		"extract-text-webpack-plugin": "^3.0.0",
		"file-loader": "^0.11.2",
		"html-webpack-plugin": "^2.30.1",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^22.1.4",
		"jest-enzyme": "^4.0.2",
		"jest-fetch-mock": "^1.4.2",
		"json-loader": "^0.5.7",
		"json-server": "^0.12.1",
		"raf": "^3.4.0",
		"react": "^16.3.2",
		"react-addons-test-utils": "^15.6.2",
		"react-test-renderer": "^16.0.0",
		"redux-mock-store": "^1.5.1",
		"style-loader": "^0.18.2",
		"uglifyjs-webpack-plugin": "^1.1.6",
		"webpack": "^3.4.1",
		"webpack-bundle-analyzer": "^3.6.1",
		"webpack-dashboard": "^1.0.2",
		"webpack-dev-server": "^2.6.1",
		"webpack-manifest-plugin": "^1.3.2",
		"webpack-merge": "^4.1.1",
		"webpack-pwa-manifest": "^3.4.1"
	},
	"jest": {
		"moduleNameMapper": {
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/assetsTransformer.js",
			"\\.(css)$": "identity-obj-proxy"
		},
		"setupFiles": [
			"raf/polyfill",
			"./tools/enzymeTestAdapterSetup.js",
			"./tools/setupJest.js"
		]
	}
}
