{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "a basic and opininated boilerplate for a react app",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "test": "jest",
    "watch": "webpack --watch",
    "build": "webpack-dev-server --open --config webpack.dev.js",
    "dashboard": "webpack-dashboard -- webpack-dev-server --open --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muneneevans/react-js-bolierplate.git"
  },
  "keywords": [
    "boilerplate"
  ],
  "author": "Evans Munene",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/muneneevans/react-js-bolierplate/issues"
  },
  "homepage": "https://github.com/muneneevans/react-js-bolierplate#readme",
  "dependencies": {
    "preact": "^8.2.7",
    "preact-compat": "^3.17.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.7.1",
    "redux-thunk": "^2.2.0",
    "seamless-immutable": "^7.1.2"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.1.4",
    "jest-enzyme": "^4.0.2",
    "json-loader": "^0.5.7",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.4.1",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-dashboard": "^1.0.2",
    "webpack-dev-server": "^2.6.1",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-merge": "^4.1.1",
    "webpack-pwa-manifest": "^3.4.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
